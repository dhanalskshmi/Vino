@using RJP.MultiUrlPicker.Models;
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    var favicon = Model.Content.AncestorOrSelf(1).GetPropertyValue<IPublishedContent>("favicon");
    var logo = Model.Content.AncestorOrSelf(1).GetPropertyValue<IPublishedContent>("siteLogo");
    var openAccountLink = Model.Content.AncestorOrSelf(1).GetPropertyValue<Link>("openAnAccount");
}
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  @Html.Partial("SeoMetaData")
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no", shrink-to-fit="no" />
  <link rel="shortcut icon" href="@favicon.Url" type="image/x-icon">
  <link href="~/css/lib/nprogress.css" rel="stylesheet" />
  <link href="~/css/lib/bootstrap.min.css" rel="stylesheet" />
  <link href="~/css/lib/jquery.mb.YTPlayer.min.css" media="all" rel="stylesheet" type="text/css">
  @RenderSection("InnerTopStyle", false)
  <link rel="stylesheet" type="text/css" href="~/css/style.min.css">
</head>
<body style="display: none">
    <div class="loaderBlock">
        <div class="loaderWrap">
            <img src="@logo.Url" alt="Manasquan Bank Loader">
        </div>
    </div>
    <div class="siteWrap">
        <div class="siteInner">
            @Html.Partial("homePopUp")
            @Html.Partial("Header")
            <div class="openAccount">
                @if (openAccountLink != null)
                {
                    if (openAccountLink.Target == "_blank")
                    {
                        <a href="@openAccountLink.Url" target="_blank">
                            <span>
                                @openAccountLink.Name
                            </span>
                        </a>
                    }
                    else
                    {
                        <a href="@openAccountLink.Url">
                            <span>
                                @openAccountLink.Name
                            </span>
                        </a>
                    }
                }

            </div>
            <div class="loginBlocks">
                <div class="personalLogin">
                    <h3>PERSONAL BANKING</h3>
                    <form id="mpersonalLogin" action="" method="post" novalidate="novalidate">
                        <label for="maccessid" class="screen-reader-text ">access id</label>
                        <input type="text" name="maccessid" id="maccessid" value="" class="txtBox" required="" placeholder="Access ID" aria-required="true">
                        <label for="password" class="screen-reader-text ">access password</label>
                        <input type="password" name="password" id="password" value="" class="txtBox" required="" placeholder="Password" aria-required="true">
                        <button type="submit" name="button"> <span>Login</span></button>
                        <p>
                            <a href="#">Sign Up</a>
                            <a href="#">Forgot Password</a>
                        </p>
                    </form>
                </div>
                <div class="businessLogin">
                    <h3>BUSINESS BANKING</h3>
                    <form id="mbusinessLogin" action="" method="post" novalidate="novalidate">
                        <label for="mbusinessaccessid" class="screen-reader-text ">business access id</label>
                        <input type="text" name="mbusinessaccessid" id="mbusinessaccessid" value="" class="txtBox" required="" placeholder="Access ID" aria-required="true">
                        <label for="mbusinesspassword" class="screen-reader-text ">business access password</label>
                        <input type="password" name="mbusinesspassword" id="mbusinesspassword" value="" class="txtBox" required="" placeholder="Password" aria-required="true">
                        <button type="submit" name="button"> <span>Login</span></button>
                        <p>
                            <a href="#">Forgot Password</a>
                        </p>
                    </form>
                </div>
            </div>
            


            @RenderBody()
            @Html.Partial("Footer")
            <!--<div class="overlay"></div>-->
        </div>
    </div>
    <script src='https://www.google.com/recaptcha/api.js' defer></script>
    <script type="text/javascript" src="~/scripts/lib/jquery3.1.0.min.js"></script>
    <!--<script type="text/javascript" src="~/scripts/lib/jquery.js"></script>-->
    <script src="~/scripts/lib/waypoints.min.js" type="text/javascript"></script>
    <script src="~/scripts/lib/nprogress.js"></script>
    <script src="~/scripts/lib/modernizr-custom.js"></script>
    <script src="~/scripts/lib/scrollanimation.js"></script>
    <!--<script src="http://remysharp.com/downloads/jquery.inview.js"></script>-->
    <script src="~/scripts/lib/jquery.mb.YTPlayer.js"></script>
    <script src="~/scripts/lib/jquery.validate.min.js"></script>
    @RenderSection("InnerBottomScript", false)
    <script type="text/javascript" src="~/scripts/main.min.js"></script>


</body>
</html>


